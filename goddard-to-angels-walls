    Delay(200);
    Engine.DlgOpen;
    Delay(300);
    Engine.DlgSel(1);
    Delay(300);
    Engine.DlgSel('Wall of Argos');
    Delay(1000);
Engine.MoveTo(164987, -47984, -3569);
Engine.MoveTo(165805, -47235, -3591);
Engine.MoveTo(166436, -46905, -3570);
Engine.MoveTo(167325, -45638, -3541);
